{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "cds serve",
            "request": "launch",
            "type": "node",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "cds",
            "args": [
                "serve",
                "--with-mocks",
                "--in-memory?"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "name": "Run VENDOR_ONBOARDING",
            "request": "launch",
            "type": "node",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "cds",
            "args": [
                "serve",
                "--in-memory?"
            ],
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "envFile": "${workspaceFolder}/.env2",
            "env": {
                "NODE_ENV": "development",
                "run.config": "{\"handlerId\":\"cap_run_config_handler_id\",\"runnableId\":\"{\\\"projectPath\\\":\\\"/home/user/projects/VENDOR_ONBOARDING\\\",\\\"profileName\\\":\\\"development\\\"}\"}"
            }
        },
        {
            "name": "Start unitcodeshellplugin",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "npx",
            "cwd": "${workspaceFolder}/unitcodeshellplugin",
            "windows": {
                "runtimeExecutable": "npx.cmd"
            },
            "runtimeArgs": [
                "fiori",
                "run"
            ],
            "args": [
                "--open",
                "test/flpSandbox.html#spfioriunitcodeshellplugin-display"
            ],
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart",
            "outputCapture": "std",
            "port": 9229,
            "env": {
                "DEBUG": "--inspect",
                "FIORI_TOOLS_URL_PARAMS": "sap-ui-xx-viewCache=false",
                "run.config": "{\"handlerId\":\"fiori_tools\",\"runnableId\":\"/home/user/projects/VENDOR_ONBOARDING/unitcodeshellplugin\"}"
            }
        },
        {
            "name": "Start unitcodeshellplugin Mock",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}/unitcodeshellplugin",
            "runtimeExecutable": "npx",
            "windows": {
                "runtimeExecutable": "npx.cmd"
            },
            "runtimeArgs": [
                "fiori",
                "run"
            ],
            "args": [
                "--config",
                "./ui5-mock.yaml",
                "--open",
                "test/flpSandbox.html#spfioriunitcodeshellplugin-display"
            ],
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart",
            "outputCapture": "std",
            "port": 9229,
            "env": {
                "FIORI_TOOLS_URL_PARAMS": "sap-ui-xx-viewCache=false",
                "run.config": "{\"handlerId\":\"fiori_tools\",\"runnableId\":\"/home/user/projects/VENDOR_ONBOARDING/unitcodeshellplugin\"}"
            }
        },
        {
            "name": "Start unitcodeshellplugin Local",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}/unitcodeshellplugin",
            "runtimeExecutable": "npx",
            "windows": {
                "runtimeExecutable": "npx.cmd"
            },
            "runtimeArgs": [
                "fiori",
                "run"
            ],
            "args": [
                "--config",
                "./ui5-local.yaml",
                "--open",
                "test/flpSandbox.html#spfioriunitcodeshellplugin-display"
            ],
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart",
            "outputCapture": "std",
            "port": 9229,
            "env": {
                "FIORI_TOOLS_URL_PARAMS": "sap-ui-xx-viewCache=false",
                "run.config": "{\"handlerId\":\"fiori_tools\",\"runnableId\":\"/home/user/projects/VENDOR_ONBOARDING/unitcodeshellplugin\"}"
            }
        }
    ]
}
<Dialog
    id="gstDialog"
    title="GST Details"
    titleAlignment="Center"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:u="sap.ui.unified"
    draggable="true"
    contentWidth="300px"
    contentHeight="350px"
    class="sapUiSizeCompact"
>
    <content>
        <f:SimpleForm
            id="_IDGenSimpleForm1"
            editable="true"
            layout="ResponsiveGridLayout"
        >
            <core:Title text="GST 1" />
            <Label
                design="Bold"
                text="GSTIN 1"
            />
            <Input
                placeholder="Enter GST Number (Eg: 29ABCDE1234ZXXX)"
                change="onInputChange($event,'GstNumber1')"
                value="{path:'GstModel>/GstNumber1',type: '.customGstType'}"
                enabled="false"
            />
            <Label
                design="Bold"
                text="Upload GSTIN 1"
            />
            <u:FileUploader
                sendXHR="true"
                useMultipart="false"
                uploadComplete="onUploadComplete"
                buttonText="Browse"
                fileSizeExceed="onFileSizeExceded"
                fileType="jpg,jpeg,msg,pdf,docx,doc,ppt,pptx,ost"
                change="onFileUploaderChange"
                maximumFileSize="10MB"
                enabled="false"
            >
                <u:customData>
                    <core:CustomData key="GstFileName1" />
                </u:customData>
            </u:FileUploader>
            <Link
                text="{GstModel>/GstFileName1}"
                press="onAttachmentGet"
                visible="{= ${GstModel>/GstFileName1} !== ''}"
            >
                <customData>
                    <core:CustomData key="GstFileName1" />
                </customData>
            </Link>
            <Label
                design="Bold"
                text="Address 1"
            />
            <Input
                placeholder="Plot No.,House No.,Street No."
                maxLength="60"
                value="{path:'GstModel>/Gst1Address1',type:'sap.ui.model.type.String'}"
                enabled="false"
            />
            <Label
                design="Bold"
                text="Address 2"
            />
            <Input
                placeholder="City, State, District"
                maxLength="40"
                value="{path:'GstModel>/Gst1Address2',type:'sap.ui.model.type.String'}"
                enabled="false"
            />

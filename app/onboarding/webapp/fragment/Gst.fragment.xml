    <beginButton>
        <Button
            id="_IDGenButton1"
            type="Emphasized"
            text="Submit"
            press="onGstSubmit"
            visible="false"
        />
    </beginButton>
    <endButton>
        <Button
            id="_IDGenButton2"
            type="Reject"
            text="Cancel"

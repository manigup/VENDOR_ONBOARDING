<Dialog
    id="createDialog"
    title="Create Supplier"
    titleAlignment="Center"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    draggable="true"
    escapeHandler=".onDialogEscapeHandler"
    contentWidth="350px"
    contentHeight="400px"
    class="sapUiSizeCompact"
>
    <content>
        <f:SimpleForm
            id="_IDGenSimpleForm1"
            editable="true"
            layout="ResponsiveGridLayout"
        >
            <Label
                id="_IDGenLabel1"
                design="Bold"
                required="true"
                text="Supplier Type"
            />
            <Select
                id="_IDGenSelect1"
                forceSelection="false"
                selectedKey="{path:'CreateModel>/VendorType',type:'sap.ui.model.type.String',constraints:{minLength:2}}"
                valueStateText="Required"
                fieldGroupIds="required"
            >
                <core:Item
                    id="_IDGenItem1"
                    key=""
                    text=""
                />
                <core:Item
                    id="_IDGenItem2"
                    key="DM"
                    text="Domestic"
                />
                <core:Item
                    id="_IDGenItem3"
                    key="IP"
                    text="Import"
                />
            </Select>
            <Label
                id="_IDGenLabel2"
                design="Bold"
                required="true"
                text="Supplier Name"
            />
            <Input
                id="_IDGenInput1"
                value="{path:'CreateModel>/VendorName',type:'sap.ui.model.type.String',constraints:{minLength:1}}"
                valueStateText="Required"
                fieldGroupIds="required"
            />
            <Label
                id="_IDGenLabel9"
                design="Bold"
                required="true"
                text="Company Code"
            />
            <Select
                id="_IDGenSelect4"
                forceSelection="false"
                selectedKey="{path:'CreateModel>/Companycode',type:'sap.ui.model.type.String'}"
                valueStateText="Required"
                fieldGroupIds="required"
            >
                <core:Item
                    id="_IDGenItem11"
                    key=""
                    text=""
                />
                <core:Item
                    id="_IDGenItem12"
                    key="1000 IMPERIAL AUTO INDUSTRIES LIMITED"
                    text="1000 IMPERIAL AUTO INDUSTRIES LIMITED"
                />
                <core:Item
                    id="_IDGenItem13"
                    key="2000 SJ RUBBER INDUSTRIES LIMITED"
                    text="2000 SJ RUBBER INDUSTRIES LIMITED"
                />
                <core:Item
                    id="_IDGenItem14"
                    key="3000 IAI INDUSTRIES LIMITED"
                    text="3000 IAI INDUSTRIES LIMITED"
                />
                <core:Item
                    id="_IDGenItem15"
                    key="4000 IMPERIAL MARTOR ENGINE TUBES PRIVATE LIMITED"
                    text="4000 IMPERIAL MARTOR ENGINE TUBES PRIVATE LIMITED"
                />
            </Select>
            <Label
                id="_IDGenLabel7"
                design="Bold"
                required="true"
                text="Registration Type"
            />
            <Select
                id="_IDGenSelect2"
                forceSelection="false"
                selectedKey="{path:'CreateModel>/RegistrationType',type:'sap.ui.model.type.String'}"
                valueStateText="Required"
                fieldGroupIds="required"
            >
                <core:Item
                    id="_IDGenItem6"
                    key="Customer Driven (Domestic)"
                    text="Customer Driven (Domestic)"
                />
                <core:Item
                    id="_IDGenItem5"
                    key="Customer Driven (Export)"
                    text="Customer Driven (Export)"
                />
                <core:Item
                    id="_IDGenItem10"
                    key="BOM Parts"
                    text="BOM Parts (Quality Approval Reqd)"
                />
                <core:Item
                    id="_IDGenItem4"
                    key="Non BOM parts"
                    text="Indirect Material"
                />
            </Select>
            <Label
                id="_IDGenLabel10"
                design="Bold"
                text="Group Type"
                required="true"
            />
            <!-- <Select
                id="grouptypeId"
                forceSelection="false"
                selectedKey="{path:'CreateModel>/GroupType',type:'sap.ui.model.type.String'}"
                valueStateText="Required"
                fieldGroupIds="required"
            > -->
            <MultiComboBox
                id="grouptypeId"
                showSecondaryValues="true"
                selectedKeys="{CreateModel>/GroupType}"
                width="500px"
                valueStateText="Required"
                fieldGroupIds="required"
            >
                <core:ListItem
                    id="_IDGenListItem1"
                    key="A01"
                    text="Casting"
                    additionalText="CASTI"
                />
                <core:ListItem
                    id="_IDGenListItem2"
                    key="B01"
                    text="Forging"
                    additionalText="FORGI"
                />
                <core:ListItem
                    id="_IDGenListItem3"
                    key="C01"
                    text="Bulk Hose"
                    additionalText="BULKH"
                />
                <core:ListItem
                    id="_IDGenListItem4"
                    key="D01"
                    text="Formed Hose"
                    additionalText="FORHO"
                />
                <core:ListItem
                    id="_IDGenListItem5"
                    key="E01"
                    text="Ind Material - Prod"
                    additionalText="INMTP"
                />
                <core:ListItem
                    id="_IDGenListItem6"
                    key="F01"
                    text="Ind Material - Other"
                    additionalText="INMTO"
                />
                <core:ListItem
                    id="_IDGenListItem7"
                    key="G01"
                    text="Molding"
                    additionalText="MOLDI"
                />
                <core:ListItem
                    id="_IDGenListItem8"
                    key="H01"
                    text="Job-work"
                    additionalText="JOBWO"
                />
                <core:ListItem
                    id="_IDGenListItem9"
                    key="I01"
                    text="Machining"
                    additionalText="MACHI"
                />
                <core:ListItem
                    id="_IDGenListItem10"
                    key="J01"
                    text="RM - Polymer"
                    additionalText="RMPOL"
                />
                <core:ListItem
                    id="_IDGenListItem11"
                    key="K01"
                    text="Proprietory"
                    additionalText="PROPR"
                />
                <core:ListItem
                    id="_IDGenListItem12"
                    key="L01"
                    text="Sheet Metal"
                    additionalText="SHEET"
                />
                <core:ListItem
                    id="_IDGenListItem13"
                    key="M01"
                    text="Capex"
                    additionalText="CAPEX"
                />
                <core:ListItem
                    id="_IDGenListItem14"
                    key="N01"
                    text="Tubes"
                    additionalText="TUBES"
                />
                <core:ListItem
                    id="_IDGenListItem15"
                    key="O01"
                    text="Sleeves"
                    additionalText="SLEEV"
                />
                <core:ListItem
                    id="_IDGenListItem16"
                    key="P01"
                    text="Springs"
                    additionalText="SPRIN"
                />
            </MultiComboBox>
            <!-- </Select> -->
            <Label
                design="Bold"
                text="Reasons for the development of new vendor"
                required="true"
            />
            <Select
                id="purposeId"
                showSecondaryValues="true"
                forceSelection="false"
                selectedKey="{path:'CreateModel>/Purpose',type:'sap.ui.model.type.String'}"
                valueStateText="Required"
                fieldGroupIds="required"
            >
                <core:Item
                    key="Customer Recommendation "
                    text="Customer Recommendation "
                />
                <core:Item
                    key="Quality Performance"
                    text="Quality Performance"
                />
                <core:Item
                    key="Make or Buy"
                    text="Make or Buy"
                />
                <core:Item
                    key="Cost"
                    text="Cost"
                />
                <core:Item
                    key="Capacity"
                    text="Capacity"
                />
                <core:Item
                    key="Other"
                    text="Other"
                />
            </Select>
            <Label
                design="Bold"
                text="Other Reason"
                required="{= ${CreateModel>/Purpose} === 'Other'}"
                visible="{= ${CreateModel>/Purpose} === 'Other'}"
            />
            <Input
                id="reasonTextId"
                visible="{= ${CreateModel>/Purpose} === 'Other'}"
                value="{path:'CreateModel>/ReasonText',type: 'sap.ui.model.type.String',constraints:{minLength: 1}}"
            />
            <Label
                id="_IDGenLabel4"
                design="Bold"
                required="true"
                text="Supplier Telephone No."
            />
            <Input
                id="_IDGenInput3"
                type="Number"
                value="{path:'CreateModel>/Telephone',type:'sap.ui.model.type.String',constraints:{minLength: 10,maxLength:10}}"
                valueStateText="Required"
                fieldGroupIds="required"
            />
            <Label
                id="_IDGenLabel5"
                design="Bold"
                required="true"
                text="City"
            />
            <Input
                id="_IDGenInput4"
                value="{path:'CreateModel>/City',type:'sap.ui.model.type.String',constraints:{minLength:1}}"
                valueStateText="Required"
                fieldGroupIds="required"
            />
            <Label
                id="_IDGenLabel6"
                design="Bold"
                required="true"
                text="Supplier Email"
            />
            <Input
                id="_IDGenInput5"
                value="{path:'CreateModel>/VendorMail',type:'.customEMailType'}"
                valueStateText="Required"
                fieldGroupIds="required"
            />

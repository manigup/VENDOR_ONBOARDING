                    key="Yes"
                    text="Yes"
                />
                <core:Item
                    id="_IDGenItem9"
                    key="No"
                    text="No"
                />
            </Select>
            <Label
                id="_IDGenLabel10"
                design="Bold"
                required="true"
                text="VDA Assessment"
            />
            <Select
                id="_IDGenSelect5"
                forceSelection="false"
                selectedKey="{path:'CreateModel>/VDAAssessment',type:'sap.ui.model.type.String'}"

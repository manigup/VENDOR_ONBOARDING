            >
                <core:Item
                    id="_IDGenItem16"
                    key="Yes"
                    text="Yes"
                />
                <core:Item
                    id="_IDGenItem17"
                    key="No"
                    text="No"
                />
            </Select> -->
        </f:SimpleForm>
    </content>
    <beginButton>
        <Button
            id="_IDGenButton1"
            type="Emphasized"
            text="Submit"
            press="onCreateSubmit"
        />
    </beginButton>
    <endButton>
        <Button
            id="_IDGenButton2"
            type="Reject"
            text="Cancel"
            press="onDialogCancel"
        />
    </endButton>
</Dialog>
